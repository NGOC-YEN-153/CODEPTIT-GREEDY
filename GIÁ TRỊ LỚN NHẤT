#include <bits/stdc++.h>
using namespace std;
#define  ll long long
void solve(){
	int n;
	cin >> n;
	ll a[n];
	for(int i  = 0 ; i< n; i++)cin>>a[i];
	ll sum  =0 ;
	for(int i =0 ; i< n; i++)if(a[i] > 0)sum+=a[i];
	cout<<2*sum<<endl;
}
int main(){
	int t = 1;
//	cin >> t;
	while(t--){
	    solve();
	}
}
